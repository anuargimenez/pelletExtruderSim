FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
"rho.*"
    {
        solver          diagonal;
    }

    "p.*"
    {
        tolerance        1e-8;
        relTol           0.01;

        solver           PCG;
        preconditioner  
        {
        preconditioner   GAMG;
        smoother         DIC;
        nPreSweeps       1;
        nPostSweeps      2;
        nFinestSweeps    2;
        nCellsInCoarsestLevel 10;
        agglomerator     faceAreaPair;
        mergeLevels      1;
        }
        minIter         3;
        maxIter         1000;
    }

    "U.*"
    {
        type             coupled;
        solver           PBiCICG;
        preconditioner   DILU;
        tolerance        (1e-8 1e-8 1e-8);
        relTol           (0.01 0.01 0.01);
    }

    "h.*"
    {
        solver           PBiCG;
        preconditioner   DILU;
        tolerance        1e-8;
        relTol           0.01;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 2;
    pMinFactor      0.99;
    pMaxFactor      200;

    transonic       no;
    consistent      no;

    rhoMax          rhoMax [ 1 -3 0 0 0 0 0 ] 0.5;
    rhoMin          rhoMin [ 1 -3 0 0 0 0 0 ] 2.0;
    
    residualControl
    {
        p               1e-6;
        U               1e-5;
        h		 1e-7;
    }
}

relaxationFactors
{
    fields
    {
        p                 0.2;
    }
    equations
    {
        U                 0.7;
        "(e|h)"           0.7;
    }
}




