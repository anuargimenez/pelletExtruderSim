#!/bin/sh
cd ${0%/*} || exit 1    # Run from this directory

# Source tutorial run functions
. $WM_PROJECT_DIR/bin/tools/RunFunctions

application=$(getApplication)

runApplication fluentMeshToFoam MRF_Pellet_Extruder_v2.msh

runApplication topoSet

decomposePar

mpirun -np 6 polymSimpleFoam -parallel > log & tail -f log # rausgenommen f√ºr Bash-Bearbeitung Scheid
 
echo "Done!"
#------------------------------------------------------------------------------
