#!/bin/sh
cd ${0%/*} || exit 1    # Run from this directory

# Source tutorial run functions
. $WM_PROJECT_DIR/bin/tools/RunFunctions

application=$(getApplication)

runApplication fluentMeshToFoam nonMRF_Pellet_Extruder.msh

decomposePar

mpirun -np 4 polymSimpleFoam -parallel > log & tail -f log # rausgenommen f√ºr Bash-Bearbeitung Scheid
 
echo "Done!"
