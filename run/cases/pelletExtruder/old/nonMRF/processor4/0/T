FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 405.65;

boundaryField
{
    wall1
    {
        type            convectiveRadiativeHeatFlux;
        mode            convectiveRadiative;
        Ta              constant 513.15;
        h               uniform 215;
        emissivityNozzle 0.95;
        emissivityFilament 0.95;
        dNozzle         4;
        dFilament       1.75;
        kappaMethod     fluidThermo;
        value           uniform 405.65;
    }
    wall2
    {
        type            fixedValue;
        value           nonuniform 
0
;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform 
0
;
    }
    outlet
    {
        type            zeroGradient;
    }
    screw
    {
        type            zeroGradient;
    }
    procBoundary4to3
    {
        type            processor;
        value           uniform 405.65;
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform 405.65;
    }
}


