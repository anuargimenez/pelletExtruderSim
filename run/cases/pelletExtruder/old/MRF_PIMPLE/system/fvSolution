FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers 
{ 
	rho
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-06;
        relTol          0.1;
    }
	rhoFinal
    {
        $rho;
        relTol          0;
    }
    p 
    { 
        solver          PCG; 
        preconditioner  DIC; 
        tolerance       1e-06; 
        relTol          0.05; 
    } 
    pFinal 
    { 
        $p; 
        relTol          0; 
    } 
    U 
    { 
        solver          smoothSolver; 
        smoother        GaussSeidel; 
        tolerance       1e-05; 
        relTol          0; 
    }
	UFinal
    {
        $U;
        relTol          0;
    }
	h 
    { 
        solver          smoothSolver; 
        smoother        GaussSeidel; 
        tolerance       1e-05; 
        relTol          0; 
    }
	hFinal
    {
        $h;
        relTol          0;
    }
    nuTilda 
    { 
        solver          smoothSolver; 
        smoother        GaussSeidel; 
        tolerance       1e-05; 
        relTol          0; 
    }
	nuTildaFinal
    {
        $nuTilda;
        relTol          0;
    } 
} 
PIMPLE 
{ 
	consistent		true;
	momentumPredictor yes;
	transonic no;
    nCorrectors     2; 
    nNonOrthogonalCorrectors 3;
	nOuterCorrectors	100;
	turbOnFinalIterOnly false;
	
	pRefCell        0; 
    pRefValue       1e5;

	pMax                1.2e5;
    pMin                0.8e5; 

	rhoMin			0.5;
	rhoMax			2;

	residualControl
	{
		U	 1e-5;
		p        5e-4;

		h         5e-4;
	} 
}


