FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    wall1
    {
        type            slip;
    }
    wall2
    {
        type            noSlip;
    }
    inlet
    {
        type            flowRateInletVelocity;
        massFlowRate    constant 3e-05;
        rhoInlet        1040;
        extrapolateProfile 1;
        value           uniform (0 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }
    screw
    {
        type            slip;
    }
    procBoundary5to4
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


// ************************************************************************* //
