FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    wedge1
    {
        type            wedge;
    }

    wedge2
    {
        type            wedge;
    }   
    
    wall1
    {
        type            slip;
    }
    
    wall2
    {
        type            noSlip;
    }
    
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    inlet
    {
       /*  type            flowRateInletVelocity;
        massFlowRate    constant 1e-05; // kg/s
        rho             rho;
        //rhoInlet        1000;    // Guess for rho */
       type		fixedValue;
       value		uniform (0 0 5);
    }
}


