FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "3e-05";
    object      thermo:mu;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0.014792;

boundaryField
{
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    input
    {
        type            calculated;
        value           uniform 0.014792;
    }
    output
    {
        type            calculated;
        value           uniform 0.014792;
    }
    wall2
    {
        type            calculated;
        value           uniform 0.014792;
    }
    wall1
    {
        type            calculated;
        value           uniform 0.014792;
    }
}


