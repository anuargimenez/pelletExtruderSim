thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       polymerK;
    thermo          hConst;
    equationOfState rhoConst;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        molWeight   150;
    }
    thermodynamics
    {
        Cp          1007;
        Hf          0;
    }
    transport
    {
        mu          1.4792e-02;
        Pr          116;
    }
     
    equationOfState
    {
        rho        1000;
    }
}

