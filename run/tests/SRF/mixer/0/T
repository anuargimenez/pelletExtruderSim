FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 405.65; //initial condition

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 298.15; // temperature ambient 25ÂºC
    }
    outlet
    {
        type            zeroGradient;
    }
    innerWall
    {
        type            fixedValue;
        value           uniform 513.15; // solo calor en la zona 2
    }
    outerWall
    {
        type            fixedValue;
        value           uniform 513.15; // solo calor en la zona 2
    }
    cyclic_half0
    {
        type            cyclic;
    }
    cyclic_half1
    {
        type            cyclic;
    }
}


